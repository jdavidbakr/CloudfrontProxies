{
    "name": "jdavidbakr/cloudfront-proxies",
    "description": "Sets proxy settings for Cloudfront in a Laravel project",
    "keywords": [
        "cloudfront",
        "laravel",
        "trusted proxies"
    ],
    "license": "MIT",
    "authors": [{
        "name": "J David Baker",
        "email": "me@jdavidbaker.com",
        "homepage": "http://www.jdavidbaker.com",
        "role": "Developer"
    }],
    "require": {
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|^9.0|^10.0",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0|~8.0|^9.0|^10.0",
        "orchestra/testbench": "~3.0|~4.0|~5.0|~6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\CloudfrontProxies\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jdavidbakr\\CloudfrontProxies\\CloudfrontProxiesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
